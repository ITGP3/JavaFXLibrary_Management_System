<AnchorPane prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: #E9D595;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="staff.StaffMainController">
   <children>
      <Label alignment="CENTER" layoutY="-1.0" prefHeight="50.0" prefWidth="700.0" text="Staff Section">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <FontAwesomeIcon glyphName="USER" layoutX="251.0" layoutY="33.0" size="25" />
      <TableView fx:id="staffTable" layoutX="4.0" layoutY="115.0" prefHeight="371.0" prefWidth="562.0">
        <columns>
          <TableColumn fx:id="staffId" prefWidth="25.0" style="-fx-background-color: #FFEAA3;" text="Id" />
          <TableColumn fx:id="staffFirstName" prefWidth="69.0" style="-fx-background-color: #F3DF9B;" text="FirstName" />
            <TableColumn fx:id="staffLastName" prefWidth="70.0" style="-fx-background-color: #FFEAA3;" text="LastName" />
            <TableColumn fx:id="staffEmail" prefWidth="96.0" style="-fx-background-color: #F3DF9B;" text="Email" />
            <TableColumn fx:id="staffPassword" prefWidth="60.0" style="-fx-background-color: #FFEAA3;" text="Password" />
            <TableColumn fx:id="staffPhone" prefWidth="69.0" style="-fx-background-color: #F3DF9B;" text="Phone" />
            <TableColumn fx:id="staffStatus" prefWidth="57.0" style="-fx-background-color: #FFEAA3;" text="Status" />
            <TableColumn fx:id="staffDOB" prefWidth="43.0" style="-fx-background-color: #F3DF9B;" text="DOB" />
            <TableColumn fx:id="staffAddress" prefWidth="76.0" style="-fx-background-color: #FFEAA3;" text="Address" />
        </columns>
         <effect>
            <InnerShadow />
         </effect>
      </TableView>
      <VBox layoutX="576.0" layoutY="115.0" prefHeight="371.0" prefWidth="122.0" spacing="11.0">
         <children>
            <Button mnemonicParsing="false" onMouseClicked="#processAdd" prefHeight="65.0" prefWidth="123.0" style="-fx-background-color: #FFFFFF;" text="Add">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" onMouseClicked="#processAdd" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../image/staffSection/add-user.png" />
                     </image>
                  </ImageView>
               </graphic>
               <effect>
                  <DropShadow />
               </effect>
            </Button>
            <Button layoutX="10.0" layoutY="30.0" mnemonicParsing="false" onMouseClicked="#processEdit" prefHeight="65.0" prefWidth="123.0" style="-fx-background-color: #FFFFFF;" text="Edit">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" onMouseClicked="#processEdit" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../image/staffSection/user.png" />
                     </image>
                  </ImageView>
               </graphic>
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
               <effect>
                  <DropShadow />
               </effect>
            </Button>
            <Button layoutX="10.0" layoutY="100.0" mnemonicParsing="false" onMouseClicked="#processView" prefHeight="65.0" prefWidth="123.0" style="-fx-background-color: #FFFFFF;" text="View">
